<?xml version="1.0"?>
<config>
  <global>
    <basedir>@@@install_dir@@@/share/babyslam/media</basedir>
  </global>
  <importdirs>~/.babyslam/sets</importdirs>
  <set id="letters">
    <item weight="40"><effect type="letter"/></item>
  </set>
  <set id="animals">
    <item>
      <effect type="rotate"/>
      <effect type="flip"/>
      <image>sample_cow.png</image>
      <sound>sample_cow.wav</sound>
    </item>
  </set>
</config>
